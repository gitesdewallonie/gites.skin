                       tal:attributes="value heb/heb_tarif_chmbr_sans_dej_3p" />
             </td>
          </tr>
          <tr>
             <td>Si table d'hôte, prix</td>
             <td align="center">
                <input type="text"
                       name="heb_tarif_chmbr_table_hote_1p"
                       size="10"
                       value=""
                       tal:attributes="value heb/heb_tarif_chmbr_table_hote_1p" />
             </td>
             <td align="center">
                <input type="text"
