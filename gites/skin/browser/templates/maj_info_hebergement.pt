                       tal:attributes="value heb/heb_tarif_chmbr_sans_dej_1p" />
             </td>
             <td align="center">
                <input type="text"
                       name="heb_tarif_chmbr_sans_dej_2p"
                       size="10"
                       value=""
                       tal:attributes="value heb/heb_tarif_chmbr_sans_dej_2p" />
             </td>
             <td align="center">
                <input type="text"
                       name="heb_tarif_chmbr_sans_dej_3p"
                       size="10"
