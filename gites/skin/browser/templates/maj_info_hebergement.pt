          <tr>
             <td colspan="4" align="left" bgcolor="#006666">
                <span class="titre_encodage">
                TARIFICATION DES CHAMBRES
                </span>
             </td>
          </tr>
          <tr>
             <td>&nbsp;</td>
             <td align="center"><b>1 pers</b></td>
             <td align="center"><b>2 pers</b></td>
             <td align="center"><b>3 pers</b></td>
          <tr bgcolor="#E0FEEA">
             <td>Chambre avec petit déjeuner</td>
             <td align="center">
                <input type="text"
                       name="heb_tarif_chmbr_avec_dej_1p"
                       size="10" 
                       value=""
                       tal:attributes="value heb/heb_tarif_chmbr_avec_dej_1p" />
             </td>
             <td align="center">
                <input type="text" 
                       name="heb_tarif_chmbr_avec_dej_2p"
                       size="10"
                       value=""
                       tal:attributes="value heb/heb_tarif_chmbr_avec_dej_2p" />
             </td>
             <td align="center">
                <input type="text"
                       name="heb_tarif_chmbr_avec_dej_3p"
                       size="10"
                       value=""
                       tal:attributes="value heb/heb_tarif_chmbr_avec_dej_3p" />
