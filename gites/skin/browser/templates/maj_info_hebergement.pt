                <dtml-if "heb_tenis=='oui'">
                   <input type="checkbox" name="fheb_tenis" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_tenis" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/a_tennis.png">
                <input type="text" 
                       name="heb_tenis_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_tenis_distance" />
                &nbsp;&nbsp;&nbsp;
