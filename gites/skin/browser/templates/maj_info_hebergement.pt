                    &nbsp;&nbsp;&nbsp;
                    Equitation
                    <br />
                    <input type="checkbox"
                           name="heb_maj_velo"
                           checked=""
                           value="oui"
                           tal:attributes="checked python:heb.heb_velo=='oui'
                                           and 'CHECKED'or None;" />
                    <img src="a_velo.png">
