                     <div>
                         <tal:boucle tal:repeat="table tableHote">
                             <input type="checkbox"
                                               name="hebhot_tabhot_fk"
                                               checked=""
                                               value=""
                                               tal:attributes="checked python:table.tabho_pk in typeTables
                                                               and table.tabho_pk in typeTables
                                                               or None;
                                                               value python:table.tabho_pk" />
                                        <span tal:condition="python: table.tabho_pk in typeTables">
                                                <img src=""
                                                     alt=""
                                                     title="Assiette gourmande"
                                                     width="47"
                                                     height="36"
                                                     tal:attributes="src string:assiette_${repeat/table/number}.png;
                                                                     alt string:assiette_${repeat/table/number}.png;
                                                                     title table/tabho_type_fr" />
                                                <br />
                                                <span tal:content="table/tabho_type_fr"
                                                      class="grasnoir" />
                                                <br /><br />
                                            </span>
                                            <span tal:condition="python: table.tabho_pk not in typeTables">
                                                <img src=""
                                                     alt=""
                                                     title="Assiette gourmande"
                                                     width="47"
                                                     height="36"
                                                     tal:attributes="src string:assiette_${repeat/table/number}_off.png;
                                                                     alt string:assiette_${repeat/table/number}.png;
                                                                     title table/tabho_type_fr" />
                                                <br />
                                                <span tal:content="table/tabho_type_fr" />
                                                <br /><br />
                                        </span>
                        </tal:boucle>
                    </div>
                 </td>
              </tr>
              <tr>
