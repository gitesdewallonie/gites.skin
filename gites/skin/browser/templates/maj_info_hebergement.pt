             <td colspan="2"></td>
          </tr>
          <tr>
             <td>Animaux</td>
             <td>
                <img src="images/picto/c_chien.png">
                <!-->
                <dtml-if "heb_animal=='oui'">
                   <input type="radio" name="fheb_animal" value="oui" checked>Oui
                   &nbsp;&nbsp;&nbsp;
                   <input type="radio" name="fheb_animal" value="non">Non
                <dtml-else>
                   <input type="radio" name="fheb_animal" value="oui">
                   &nbsp;&nbsp;&nbsp;
                   <input type="radio" name="fheb_animal" value="non" checked>Non
                </dtml-if>
                -->
                   &nbsp;&nbsp;&nbsp;
                </td>
          </tr>
          <tr>
             <td colspan="2"></td>
          </tr>
          <tr>
             <td>Fumeur</td>
             <td>
                <img src="images/picto/c_fumeur.png">
                <!-->
                <dtml-if "heb_fumeur=='oui'">
                   <input type="radio" name="fheb_fumeur" value="oui" checked>Oui
                   &nbsp;&nbsp;&nbsp;
                   <input type="radio" name="fheb_fumeur" value="non">Non
                <dtml-else>
                   <input type="radio" name="fheb_fumeur" value="oui">
                   &nbsp;&nbsp;&nbsp;
                   <input type="radio" name="fheb_fumeur" value="non" checked>Non
                </dtml-if>
                -->
                   &nbsp;&nbsp;&nbsp;
                </td>
          </tr>

