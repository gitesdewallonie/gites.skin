              <tr bgcolor="#FDFFE0">
                 <td class="maj-etiquette">Fumeur</td>
                 <td>
                     <input type="checkbox"
                            name="heb_maj_fumeur"
                            checked=""
                            value="oui"
                            tal:attributes="checked python:heb.heb_fumeur=='oui'
                                            and 'CHECKED' or '';" />
                       <img src="c_fumeur.png">
                       &nbsp;&nbsp;&nbsp;
                    </td>
              </tr>
           </table>
           <!---[FIN INFORMATION GENERALE]--->


           <!---[DEBUT TARIFICATION CHAMBRE]--->
           <table class="maj-info"
                  tal:condition="python: heb.type.type_heb_pk in (5,6,9)">
              <tr>
                 <td colspan="4" align="left" bgcolor="#FF862B">
                    <span class="titre_encodage">
                    TARIFICATION DES CHAMBRES
                    </span>
                 </td>
              </tr>
              <tr>
                 <td>&nbsp;</td>
                 <td align="center"><b>1 pers</b></td>
                 <td align="center"><b>2 pers</b></td>
                 <td align="center"><b>3 pers</b></td>
              <tr bgcolor="#FDFFE0">
                 <td class="maj-etiquette">Chambre avec petit déjeuner</td>
                 <td align="center">
                    <input type="text"
                           name="heb_maj_tarif_chmbr_avec_dej_1p"
                           size="10"
                           value=""
                           tal:attributes="value heb/heb_tarif_chmbr_avec_dej_1p" />
                 </td>
                 <td align="center">
                    <input type="text"
                           name="heb_maj_tarif_chmbr_avec_dej_2p"
                           size="10"
                           value=""
                           tal:attributes="value heb/heb_tarif_chmbr_avec_dej_2p" />
                 </td>
                 <td align="center">
                    <input type="text"
                           name="heb_maj_tarif_chmbr_avec_dej_3p"
                           size="10"
                           value=""
                           tal:attributes="value heb/heb_tarif_chmbr_avec_dej_3p" />
                 </td>
              </tr>
              <tr>
                 <td class="maj-etiquette">Déduction si pas petit déjeuner <b>(-)</b></td>
                 <td align="center">
                    <input type="text"
                           name="heb_maj_tarif_chmbr_sans_dej_1p"
                           size="10"
                           value=""
                           tal:attributes="value heb/heb_tarif_chmbr_sans_dej_1p" />
