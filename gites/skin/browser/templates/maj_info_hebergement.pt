                <input type="text" 
                       name="fheb_piscine_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_piscine_distance" />
                &nbsp;&nbsp;&nbsp;
                <!--
                <dtml-if "heb_peche=='oui'">
                   <input type="checkbox" name="fheb_peche" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_peche" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/a_peche.png">
                <input type="text" 
                       name="fheb_peche_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_peche_distance" />
                &nbsp;&nbsp;&nbsp;
                <br/>
                <!--
                <dtml-if "heb_equitation=='oui'">
                   <input type="checkbox" name="fheb_equitation" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_equitation" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/a_cheval.png">
                <input type="text" 
                       name="fheb_equitation_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_equitation_distance" />
                &nbsp;&nbsp;&nbsp;
                <!--
                <dtml-if "heb_velo=='oui'">
                   <input type="checkbox" name="fheb_velo" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_velo" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/a_velo.png">
                <input type="text" 
                       name="fheb_velo_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_velo_distance" />
                &nbsp;&nbsp;&nbsp;
                <!--
                <dtml-if "heb_vtt=='oui'">
                   <input type="checkbox" name="fheb_vtt" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_vtt" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/a_vtt.png">
                <input type="text"
                       name="fheb_vtt_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_vtt_distance" />
                &nbsp;&nbsp;&nbsp;
                <!--
                <dtml-if "heb_ravel=='oui'">
                   <input type="checkbox" name="fheb_ravel" value="oui" checked>
                <dtml-else>
                      <input type="checkbox" name="fheb_ravel" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/a_ravel.png">
                <input type="text"
                       name="fheb_ravel_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_ravel_distance" />
                &nbsp;&nbsp;&nbsp;
                <br/>
                </td>
