                       <!--
                <dtml-if "heb_commerce=='oui'">
                   <input type="checkbox" name="fheb_commerce" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_commerce" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/i_commerce.png" title="COMMERCE" />
                <input type="text"
                       name="fheb_commerce_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_commerce_distance" />
                &nbsp;&nbsp;&nbsp;
                <!--
                <dtml-if "heb_restaurant=='oui'">
                   <input type="checkbox" name="fheb_restaurant" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_restaurant" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/i_restaurant.png" title="RESTAURANT" />
                <input type="text"
                       name="fheb_restaurant_distance"
                       size="3"
                       value=""
                       tal:attributes="value heb/heb_restaurant_distance" />
                &nbsp;&nbsp;&nbsp;
                <!--
                <dtml-if "heb_gare=='oui'">
                   <input type="checkbox" name="fheb_gare" value="oui" checked>
                <dtml-else>
                   <input type="checkbox" name="fheb_gare" value="oui">
                </dtml-if>
                -->
                <img src="images/picto/i_gare.png" title="GARE" />
                <input type="text"
                       name="heb_gare_distance"
                        size="3"
                       value=""
                       tal:attributes="value heb/heb_gare_distance">
                &nbsp;&nbsp;&nbsp;
