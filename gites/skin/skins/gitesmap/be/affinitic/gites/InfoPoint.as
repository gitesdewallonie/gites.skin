package be.affinitic.gites{	import flash.display.MovieClip;	public class InfoPoint extends MovieClip	{		private var _title : String;		private var _pk : Number;		private var _type : String;		private var _localite : String;		public function InfoPoint()		{			}		public function set title(sId:String):void {			_title=sId;		}		public function get title():String {			return _title;		}		public function set pk(nId:Number):void {			_pk=nId;		}		public function get pk():Number {			return _pk;		}				public function get type():String		{			return _type;		}				public function set type(sId:String):void		{			_type=sId;		}		public function set localite(sId:String):void		{			_localite=sId;		}				public function get localite():String		{			return _localite;		}			}}