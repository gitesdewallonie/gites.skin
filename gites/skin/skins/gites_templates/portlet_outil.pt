<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="plone">
<body>
<div metal:define-macro="portlet">
    <div id="portlet-outil"
         tal:define="view context/@@logo_view">
       <div>
           <a href=""
              tal:attributes="href string:${context/@@plone_portal_state/portal_url}/preparer-sejour">
               <img src=""
                    tal:replace="structure python:view.getButton('outil_preparer_sejour.gif')"/>
           </a>
       </div>
       <div>
           <a href=""
              tal:attributes="href string:${context/@@plone_portal_state/portal_url}/decouvrir-wallonie">
               <img src=""
                    tal:replace="structure python:view.getButton('outil_decouvrir_wallonie.gif')"/>
           </a>
       </div>
       <div>
           <a href=""
              tal:attributes="href string:${context/@@plone_portal_state/portal_url}/association/devenir-membre">
               <img src=""
                    tal:replace="structure python:view.getButton('outil_proposer_hebergement.gif')"/>
           </a>
       </div>
       <div>
           <a href=""
              tal:attributes="href string:${context/@@plone_portal_state/portal_url}/coin-presse">
               <img src=""
                    tal:replace="structure python:view.getButton('outil_coin_presse.gif')"/>
           </a>
       </div>
    </div>
</div>
</body>
</html>
