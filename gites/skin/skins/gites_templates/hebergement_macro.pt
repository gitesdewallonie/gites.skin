<tal:main metal:define-macro="hebPhoto">
   <div>
      <img src=""
           tal:attributes="src string:${here/photos_heb/absolute_url}/${heb/heb_code_gdw}00.jpg" />
   </div>
</tal:main>

<tal:main metal:define-macro="hebCoordonneeChambre">
   <div id="heb_coordonnee_chambre"
        tal:condition="python:heb.type.type_heb_code in 'CH,MH,CHECR'">
      <h1 tal:content="heb/heb_nom" id="heb_nom"/><br />
      <span tal:content="heb/heb_adresse" /><br />
      <span tal:content="heb/commune/com_cp" />
      <span tal:content="heb/heb_localite" />
      (<span tal:content="heb/commune/com_nom" />)
   </div>
</tal:main>

<tal:main metal:define-macro="hebCoordonneeGite">
   <div id="heb_coordonnee_gite"
        tal:condition="python:heb.type.type_heb_code not in 'CH, MH, CHECR'">
      <span tal:content="heb/heb_nom" id="heb_nom"/><br />
      <span tal:content="heb/heb_adresse" /><br />
      <span tal:content="heb/commune/com_cp" />
      <span tal:content="heb/heb_localite" />
      (<span tal:content="heb/commune/com_nom" />)
   </div>
</tal:main>

<tal:main metal:define-macro="hebBoutonOutil">
